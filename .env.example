# CodeSage-Scrape Configuration

# Scraping Settings
MAX_CONCURRENCY=3
DELAY_BETWEEN_REQUESTS=1000
TIMEOUT=30000
RETRIES=3
HEADLESS=true

# Output Settings
OUTPUT_DIR=./data
EXPORT_FORMAT=json
INCLUDE_METADATA=true

# Debug Settings
DEBUG=false

# LeetCode Settings (if needed for specific endpoints)
# LEETCODE_USERNAME=your_username
# LEETCODE_PASSWORD=your_password

# Rate Limiting
REQUESTS_PER_MINUTE=30
BURST_LIMIT=10

# Data Collection Limits
MAX_PROBLEMS=100
MAX_SOLUTIONS_PER_PROBLEM=5
MAX_COMMENTS_PER_PROBLEM=20
